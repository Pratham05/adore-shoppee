adore-shoppe/src/components/Layout/Navbar/Navbar.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/Layout/Footer/Footer.js",[],["203","204"],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/Layout/Footer/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/Layout/Navbar/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/pages/DetailView.js",["205"],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/ImageViewer/ImageViewer.js",["206","207","208"],"import React, { useState, useEffect } from 'react';\nimport styles from './ImageViewer.module.scss';\n\nimport SampleImage from '../../../assets/images/sampleProduct.jpeg';\nimport Logo from '../../../assets/images/logo.svg';\n\n// Array of full image links\n//const images = [SampleImage, Logo, SampleImage, Logo]; \n\nconst ImageViewer = ({images}) => {\n    // UI State relevant to only this component\n    // Stores the path of the image to be displayed in big size\n    // Changes whenever a smaller image is hovered on\n    console.log(\"Resdd\", images);\n    const [bigImage, setBigImage] = useState(images[0]);\n\n\n    const onMouseOverHandler = (event) => {\n        setBigImage(event.target.src);\n    }\n\n    // This variable is used to give the active class name to the image which is selected\n    let smallImageClass = styles.ImageViewer__Active;\n\n    const smallImages = images.map((image, index) => {\n        return <img className={image === bigImage ? smallImageClass : \"\"} \n                    onMouseOver={onMouseOverHandler} \n                    src={image} key={index} alt={\"Small\" + index}/>\n    });\n\n    console.log(smallImages[0])\n\n    return (\n        <div className={styles.ImageViewer}>\n            <div className={styles.ImageViewer__ImgSmall}>\n                {smallImages}\n            </div>\n            <div className={styles.ImageViewer__ImgBig}>\n                <img src={bigImage} alt=\"Large\"/>\n            </div>\n        </div>\n    );\n};\n\nexport default ImageViewer;\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/Pagination/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/ProductPanel/ProductPanel.js",["209"],"import React, { useEffect } from 'react';\nimport styles from './ProductPanel.module.scss';\nimport ProductCard from '../../UI/ProductCard';\nimport Pagination from '../../UI/Pagination';\n\nimport { connect } from 'react-redux';\nimport * as actions from '../../../store/actions';\n\nimport { withRouter } from 'react-router-dom';\n\nimport Loading from '../../UI/Loading';\n\nimport LoadError from '../../UI/LoadError';\n\n\nconst ProductPanel = (props) => {\n\n    useEffect(()=> {\n        props.loadProducts(props.categoryIdString, props.page, props.limit);\n    }, [props.categoryIdString, props.page, props.totalPages]);\n\n    const onPageChange = (pageNumber) => {\n        let query = new URLSearchParams(props.history.location.search);\n        let params = new URLSearchParams(query);\n        params.set('page', pageNumber);\n        props.history.push('/list?' + params.toString());\n        props.changePage(pageNumber);\n    }\n\n\n    const onProductClickHandler = (id) => {\n        // Send to Product Page\n        props.history.push('/product?id=' + id);\n    };\n\n    let renderCard = null;\n    \n    if (props.loading) {\n        renderCard = <Loading/>\n    } else {\n        if (props.error) {\n            renderCard = <LoadError error={\"The Products could not be loaded! Please try refreshing the page!\"} />\n        } else {\n            renderCard = props.products.map(product=>{\n                return <ProductCard \n                            onClickHandler={() => onProductClickHandler(product.id)}\n                            key={product.id} \n                            title={product.name}\n                            price={product.price}\n                            sku={product.sku}\n                       />;\n            });\n        }\n    }\n    \n    \n    return (\n        <div className={styles.ProductPanel}>\n            <div className={styles.ProductPanel__Categories}>\n                Categories: <span>{props.categoryString.split(',').join(' | ')}</span> \n            </div>\n            <div className={styles.ProductPanel__Cards}>\n                {renderCard}\n            </div>\n            <div className={styles.ProductPanel__Pagination}>\n                <Pagination\n                onPageChangeHandler={onPageChange}\n                pageCount={props.totalPages}\n                currentPage={props.page}/>\n            </div>\n        </div>\n        \n    );\n};\n\nconst mapStateToProps = state => {\n    return {\n        products: state.productList.productList,\n        loading: state.productList.loading,\n        error: state.productList.error,\n        categoryString: state.filter.categoryNameString,\n        categoryIdString: state.filter.categoryIdString,\n        page: state.pagination.page,\n        limit: state.pagination.limit,\n        totalPages: state.productList.totalPages\n    };\n};\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        loadProducts: (categoryIdString, page, limit) => dispatch(actions.loadProductList(categoryIdString, page, limit)),\n        changePage: (page) => dispatch(actions.changePage(page))\n    };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(withRouter(ProductPanel));\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/ProductCard/index.js",[],["210","211"],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/ProductCard/ProductCard.js",["212"],"import React from 'react';\nimport styles from './ProductCard.module.scss';\nimport sampleImage from '../../../assets/images/sampleProduct.jpeg';\n\nconst ProductCard = ({img, title, price, sku, onClickHandler, productId}) => {\n    return (\n        <div className={styles.ProductCard} onClick={onClickHandler} productid={productId}>\n            {/* <img src={sampleImage} className={styles.ProductCard__Image}/> */}\n            <div className={styles.ProductCard__Description}>\n                <h2 className={styles.ProductCard__Heading}>\n                    {title}\n                </h2>\n                <h3 className={styles.ProductCard__Price}>\n                    ${price}\n                </h3>\n                <h3 className={styles.ProductCard__SKU}>\n                    SKU - {sku}\n                </h3>\n            </div>\n        </div>\n    );\n};\n\nexport default ProductCard;\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/Logo/Logo.js",["213"],"import React from 'react';\nimport logoImage from '../../../assets/images/logo.svg';\n\nconst Logo = () => {\n    return (\n        <img src={logoImage} style={{width: \"100%\"}}/>\n    );\n};\n\nexport default Logo;\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/Logo/index.js",[],["214","215"],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/Filters/FilterPanel/FilterPanel.js",["216","217"],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/ImageViewer/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/Filters/CategoryList/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/Filters/CategoryList/CategoryList.js",["218"],"import React, { useState } from 'react'\nimport styles from './CategoryList.module.scss';\n\nconst CategoryList = ({onClickHandler, categories}) => {\n\n    const normalStyle = styles.CategoryList__List__Item;\n    const activeStyle = styles.CategoryList__List__Item + \" \" + styles.CategoryList__Active;\n\n    const renderCategoryList = categories.map((category, index) => {\n        return <li \n                onClick={() => onClickHandler(index)}\n                key={index} \n                // Value Keeps track of the Button which is pressed\n                value={index}\n                className={category.isActive ? activeStyle : normalStyle}>\n                    {category.name}\n               </li>;\n    });\n\n    return (\n        <div className={styles.CategoryList}>\n            <h2 className={styles.CategoryList__Heading}>\n                categories\n            </h2>\n            <ul className={styles.CategoryList__List}>\n                { renderCategoryList }\n            </ul>\n        </div>\n    )\n}\n\n\n\nexport default CategoryList;\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/CONSTANTS.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/DetailPanel/DetailPanel.js",["219"],"import React, { useEffect } from 'react';\nimport styles from './DetailPanel.module.scss';\n\nimport { connect } from 'react-redux';\n\nimport * as actions from '../../../store/actions';\n\nimport { withRouter } from 'react-router-dom'\n\nimport Loading from '../../UI/Loading';\nimport LoadError from '../../UI/LoadError';\nimport ProductDetails from '../../UI/ProductDetails';\n\nimport ImageViewer from '../../UI/ImageViewer';\n\nconst DetailPanel = (props) => {\n\n    useEffect(()=> {\n        let query = new URLSearchParams(props.history.location.search);\n        if(query.has('id')) {\n            // The Products should now be loaded\n            props.loadProduct(query.get('id'));\n            \n        } else {\n            // ID not found, redirect to error page\n            props.history.push('/error');\n        }  \n    }, [props.history.query]);\n\n    let renderComp = null;\n\n    let imgComp = null;\n\n    if (props.loading) {\n        renderComp = <Loading />;\n    } else {\n        if (props.error) {\n            renderComp = <LoadError error={\"Unfortunately The Product could not be Loaded!\"}/>\n        } else {\n            renderComp = <ProductDetails \n                            title={props.product.title}\n                            price={props.product.price}\n                            sku={props.product.sku}\n                            description={props.product.description}\n                            categories={props.categoryString}\n                         />;\n            imgComp = <div className={styles.DetailPanel__ImageViewer}>\n                        <ImageViewer images={props.images}/>\n                    </div>;\n        }\n    }\n\n    return (\n        <div className={styles.DetailPanel}>\n            {imgComp}\n            {renderComp}\n        </div>\n    );\n};\n\nconst mapStateToProps = state => {\n    return {\n        product: state.singleProduct.product,\n        loading: state.singleProduct.loading,\n        error: state.singleProduct.error,\n        images: state.singleProduct.imageSources,\n        categoryString: state.filter.categoryString,\n    };\n};\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        loadProduct: (id) => dispatch(actions.loadProduct(id))\n    };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(withRouter(DetailPanel));\n","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/DetailPanel/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/reducers/productList.js",["220"],"import * as actionTypes from '../actions/actionTypes';\n\nconst initialState = {\n    totalPages: 0,\n    productList: [],\n    loading: false,\n    error: false,\n}\n\n\n\nconst reducer = (state=initialState, action) => {\n    switch(action.type) {\n        case actionTypes.LOAD_PRODUCT_LIST_START:\n            return {\n                ...state,\n                productList: [...state.productList],\n                loading: true\n            }\n        \n        case actionTypes.LOAD_PRODUCT_LIST_FAIL:\n            return {\n                ...state,\n                productList: [...state.productList],\n                loading: false,\n                error: true\n            }\n        \n        case actionTypes.LOAD_PRODUCT_LIST_SUCCESS:\n            return {\n                ...state,\n                loading: false,\n                error: false,\n                productList: action.productData,\n                totalPages: action.totalPages\n            }\n    }\n    return state;\n};\n\n\nexport default reducer;","/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/Filters/FilterPanel/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/reducers/singleProduct.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/actions/productList.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/actions/actionTypes.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/actions/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/pages/ErrorView.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/Loading/Loading.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/actions/singleProduct.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/LoadError/LoadError.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/ProductDetails/ProductDetails.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/LoadError/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/PageSections/ProductPanel/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/components/UI/Loading/index.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/reducers/filters.js",[],"/Users/pratham05/Desktop/AdoreBeauty/adore-shoppe/src/store/actions/filters.js",["221"],"import * as actionTypes from './actionTypes';\nimport axios from 'axios';\nimport { categoryInitials } from '../../CONSTANTS';\n\n\n\nexport const toggleCategories = (changeIndex) => {\n    return {\n        type: 